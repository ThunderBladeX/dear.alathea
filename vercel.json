{
  "builds": [
    {
      "src": "run.py",
      "use": "@vercel/python",
      "config": {
        "wsgi": "app"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/optimize_image/(.*)",
      "dest": "run.py",
      "headers": {
        "Cache-Control": "public, max-age=86400"
      }
    },
    {
      "src": "/(.*)",
      "dest": "run.py"
    }
  ],
  "env": {
    "FLASK_APP": "run.py",
    "FLASK_ENV": "production"
  }
}
